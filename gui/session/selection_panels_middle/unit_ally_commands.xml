<?xml version="1.0" encoding="utf-8"?>
<!-- Unit Commands -->
<object name="unitAllyCommandPanel" size="0 100%-34 100% 100%-4" type="image" z="30">
	<object size="0 0 100% 100%">
		<repeat count="2">
			<object name="unitAllyCommandButton[n]" hidden="true" style="iconButton" type="button" size="0 0 30 30" tooltip_style="sessionToolTipBottomBold">
				<object name="unitAllyCommandIcon[n]" ghost="true" type="image" size="0 0 100% 100%" style="commandIcon"/>
				<object name="unitAllyCommandCount[n]" ghost="true" style="groupIconsText" type="text" size="0 0 100% 100%"/>
			</object>
		</repeat>
	</object>
	<!-- shading for unit commands area -->
	<object z="50" size="4 100% 100%-4 100%" ghost="true" type="image" sprite="bottomEdgedPanelShader"/>
</object>
